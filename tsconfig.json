{
    "compilerOptions": {
      "sourceMap": true,
      "noImplicitAny": false,
      "strictFunctionTypes":true,
      "target":"es6",
      "jsx": "react",
      "allowJs":true,
      "module":"esnext",
      "moduleResolution": "node",
      "baseUrl":".",
      "declaration":false,
      "noUnusedParameters": true,
      "noUnusedLocals": true,
      "strictNullChecks": true,
      "paths": {
      },      
      "allowSyntheticDefaultImports":true,
      "lib": [
          "es2016",
          "dom",
          "es5"
      ]

    },
    "include": ["src"],
    "exclude": ["node_modules", "lib", "es"]
  }
  